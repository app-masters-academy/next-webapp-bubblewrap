{
  "name": "next-webapp-bublewrap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "bubblewrap:init": "bubblewrap init --manifest=https://cdd4e7f8c17b.ngrok.io/manifest.json --directory=./android",
    "bubblewrap:build": "cd android && bubblewrap build",
    "bubblewrap:update": "cd android && bubblewrap update --manifest=./twa-manifest.json",
    "bubblewrap:install": "cd android && bubblewrap install",
    "bubblewrap:validate": "bubblewrap validate --url=https://cdd4e7f8c17b.ngrok.io"
  },
  "dependencies": {
    "next": "10.2.0",
    "next-pwa": "^5.2.16",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.4",
    "typescript": "^4.2.4"
  }
}